<?xml version="1.0" encoding="utf-8" ?>
<litepal>
    <!--指定数据库名-->
    <dbname value="Test"/>
    <!--指定数据库版本号-->
    <version value="1"/>
    <!--指定映射模型-->
    <list>
        <!--这里使用<mapping>标签声明要配置的映射模型类，任意进行数据库操作，就会自动创建该数据库Test-->
        <mapping class="com.zhounan.winter.db.Student"/>
    </list>
</litepal>